          "_styles": {
            "&.menu": {
              "min-height": "0px",
              "padding": "0"
            },
            ".navbar": {
              "display": "flex",
              "flex-direction": "column",
              "align-items": "center",
              "min-height": "80px",
              "& when (@transparent)": {
