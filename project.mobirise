        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "&.menu": {
              "min-height": "0px",
              "padding": "0"
            },
            ".navbar": {
              "display": "flex",
              "flex-direction": "column",
              "align-items": "center",
              "min-height": "80px",
              "& when (@transparent)": {
                "& when (@transparentValue = \"50\")": {
                  "background-color": "fade(@menuBgColor, 50%)"
                },
                "& when (@transparentValue = \"100\")": {
                  "background": "transparent"
                }
              },
              "& when not (@transparent)": {
                "background": "@menuBgColor"
              },
              "& when (@sticky)": {
                "position": "fixed"
              },
              "img when (@showLogo)": {
                "height": "100%"
              },
              ".navbar-collapse": {
                "z-index": "1",
                "min-width": "250px"
              },
              "&.opened": {
                "transition": "all .3s",
                "background": "@menuBgColor !important"
              },
              "&.collapsed when (@collapsed)": {
                "flex-direction": "row",
                "button.navbar-toggler": {
                  "display": "block !important"
                },
                ".navbar-collapse": {
                  "display": "none",
                  "position": "absolute",
                  "top": "0",
                  "right": "0",
                  "min-height": "100vh",
                  "background": "@menuBgColor",
                  "padding": "1.5rem 2rem 1rem",
                  "& when (@sidebarSide = 'left')": {
                    "right": "auto",
                    "left": "0"
                  }
                },
                ".dropdown-menu": {
                  "position": "relative",
                  "background": "transparent !important",
                  ".dropdown-item": {
                    "text-align": "center",
                    "margin": "0",
                    "&:after": {
                      "right": "auto"
                    }
                  }
                },
                "& when not (@showLogo) and not (@showBrand)": {
                  "justify-content": "flex-end"
                },
                "&.opened .navbar-collapse.show, &.opened .navbar-collapse.collapsing, .ampstart-btn, amp-sidebar": {
                  "display": "block"
                },
                "@media (max-width: 991px)": {
                  ".navbar-collapse": {
                    "background": "@menuBgColor",
